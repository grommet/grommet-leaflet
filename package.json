{
  "name": "grommet-leaflet",
  "version": "0.5.1",
  "description": "Map for Grommet and React using LeafletJS",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/grommet-leaflet.cjs",
  "module": "./dist/grommet-leaflet.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.ts",
        "default": "./dist/grommet-leaflet.js"
      },
      "require": {
        "types": "./src/index.d.ts",
        "default": "./dist/grommet-leaflet.cjs"
      }
    }
  },
  "authors": [
    "Matt Glissmann",
    "Taylor Seamans",
    "Jessica Filben"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "release-stable": "node ./tools/release-stable.mjs"
  },
  "peerDependencies": {
    "grommet": "^2.32.0",
    "react": "^16.6.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.6.1 || ^17.0.0 || ^18.0.0",
    "styled-components": "^5.1.0"
  },
  "dependencies": {
    "@react-leaflet/core": "^2.1.0",
    "grommet-icons": "^4.11.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "prop-types": "^15.8.1",
    "protomaps-leaflet": "^4.0.0",
    "react-leaflet": "^4.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/leaflet": "^1.9.6",
    "@types/leaflet.markercluster": "^1.5.2",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "del": "^7.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-grommet": "https://github.com/grommet/eslint-plugin-grommet/tarball/stable",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "fs-extra": "^11.2.0",
    "grommet": "^2.32.0",
    "jsdom": "^24.1.1",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "react": "^16.6.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.6.1 || ^17.0.0 || ^18.0.0",
    "react-is": "^16.8.0",
    "rollup": "^4.19.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "simple-git": "^3.25.0",
    "styled-components": "^5.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.5"
  }
}
