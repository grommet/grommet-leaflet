    "react-router-dom": "^6.13.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "prop-types": "^15.8.1",
    "vite": "^4.1.4"
