{
  "name": "demo-app",
  "version": "0.1.0",
  "description": "Example of using leafletjs.com maps with react and grommet",
  "repository": "https://github.com/grommet/grommet-leaflet.git",
  "authors": [
    "Eric Soderberg <eric.soderberg@hpe.com>",
    "Matt Glissmann",
    "Taylor Seamans",
    "Jessica Filben"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "yarn build-grommet-leaflet && vite",
